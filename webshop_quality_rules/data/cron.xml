<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cron_validate_products" model="ir.cron">
            <field name="name">Webshop: Valideer producten voor publicatie</field>
            <field name="model_id" ref="product.model_product_template"/>
            <field name="state">code</field>
            <field name="code">model.cron_validate_products()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="active" eval="True"/>
        </record>

        <record id="cron_archive_no_suppliers" model="ir.cron">
            <field name="name">Webshop: Archiveer producten zonder leveranciers</field>
            <field name="model_id" ref="product.model_product_template"/>
            <field name="state">code</field>
            <field name="code">model.cron_archive_products_without_suppliers()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
